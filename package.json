{
  "name": "papers-generator",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"npx tailwindcss -i ./src/index.css -o ./src/build.css --watch\"",
    "build": "npx tailwindcss -i ./src/index.css -o ./src/build.css --minify && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "html2canvas": "1.3.2",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "7.1.3",
    "nanoid": "^5.0.7",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-helmet": "6.1.0",
    "react-markdown": "7.1.0",
    "remark-breaks": "3.0.2",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/line-clamp": "0.3.0",
    "@tailwindcss/typography": "0.5.0",
    "@types/lodash": "^4.17.9",
    "@types/markdown-to-jsx": "7.0.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react-refresh": "1.3.1",
    "autoprefixer": "10.4.0",
    "concurrently": "6.4.0",
    "postcss": "^8.4.47",
    "tailwindcss": "3.0.2",
    "typescript": "4.1.2",
    "vite": "^2.9.18"
  }
}
